@model IEnumerable<SmsServer.Models.Team>

@{
    ViewBag.Title = "ListTeamsForRace";
}

<h2>ListTeamsForRace</h2>

<p>
    @Html.ActionLink("Create New", "AdminCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
